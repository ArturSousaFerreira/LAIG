<SCENE>

	<INITIALS>
		<frustum near="1.5" far="500" />
		<translation x="0" y="0" z="0" />
		<rotation axis="x" angle="0" />
		<rotation axis="y" angle="0" />
		<rotation axis="z" angle="0" />
		<scale sx="1" sy="1" sz="1" />
		<reference length="6" />
	</INITIALS>


	<ANIMATIONS>
		<ANIMATION id="flag_animation" span="10" type="linear">
			<CONTROLPOINT xx="0" yy="-44" zz="0" />
			<CONTROLPOINT xx="0" yy="0" zz="0" />
		</ANIMATION>
		<ANIMATION id="paperPlane_animation" span="4" type="linear">
			<CONTROLPOINT xx="0" yy="-7" zz="0" />
			<CONTROLPOINT xx="0" yy="0" zz="0" />
		</ANIMATION>
		<ANIMATION id="circular" span="3" type="circular" center="0 0 0" radius="0" startang="160" rotang="-430"/>
	</ANIMATIONS>


	<ILLUMINATION>
        <ambient r="0" g="0" b="0" a="1" />
        <background r="0.6" g="0.6" b="0.6" a="1" />
    </ILLUMINATION>


	<LIGHTS>
        <LIGHT id="light_default">
            <enable value="1" />
            <position x="5" y="60" z="5" w="1" />
            <ambient r="0.5" g="0.5" b="0.5" a="1" />
            <diffuse r="0.5" g="0.5" b="0.5" a="1" />
            <specular r="0.5" g="0.5" b="0.5" a="1" />
        </LIGHT>
		<LIGHT id="light_lamp">
            <enable value="1" />
            <position x="0.7" y="3.4" z="0.7" w="1" />
            <specular r="1.0" g="0.92" b="0" a="1.0" />
            <diffuse r="0.3" g="0.25" b="0" a="1.0" />
            <ambient r="0.1" g="0.08" b="0" a="1.0" />
        </LIGHT>        
    </LIGHTS>


	<TEXTURES>
		<TEXTURE id="wall_pattern">
            <file path="textures/wall_paper.jpg" />
            <amplif_factor s="1" t="1" />
        </TEXTURE>
        <TEXTURE id="floor_pattern">
            <file path="textures/floor_dirt.jpg" />
            <amplif_factor s="1" t="1" />
        </TEXTURE>
        <TEXTURE id="chair_pattern">
            <file path="textures/bordeaux.jpg" />
            <amplif_factor s="1" t="1" />
        </TEXTURE>
        <TEXTURE id="lamp_top_pattern">
            <file path="textures/beje.jpg" />
            <amplif_factor s="1" t="1" />
        </TEXTURE>
        <TEXTURE id="lamp_support_pattern">
            <file path="textures/brown.jpg" />
            <amplif_factor s="1" t="1" />
        </TEXTURE>
        <TEXTURE id="painting1">
            <file path="textures/ogrito.jpg" />
            <amplif_factor s="1" t="1" />
        </TEXTURE>
        <TEXTURE id="painting2">
            <file path="textures/mona_lisa.jpg" />
            <amplif_factor s="1" t="1" />
        </TEXTURE>
        <TEXTURE id="painting3">
            <file path="textures/salvadordali.jpg" />
            <amplif_factor s="1" t="1" />
        </TEXTURE>
        <TEXTURE id="paper">
            <file path="textures/paper.jpg" />
            <amplif_factor s="1" t="1" />
        </TEXTURE>
        <TEXTURE id="flag">
            <file path="textures/flag_france.jpg" />
            <amplif_factor s="1" t="1" />
        </TEXTURE>
        <TEXTURE id="steel">
            <file path="textures/steel.jpg" />
            <amplif_factor s="1" t="1" />
        </TEXTURE>
    </TEXTURES>


   	<MATERIALS>
        <MATERIAL id="default">
            <shininess value="100" />
            <specular r="1.0" g="1.0" b="1.0" a="0.0" />
            <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />
            <ambient r="1.0" g="1.0" b="1.0" a="1.0" />
            <emission r="0" g="0" b="0" a="0" />
        </MATERIAL>
    </MATERIALS>


	<LEAVES>
		<LEAF id="rectangle" type="rectangle" args="0 0 1 1" />
		<LEAF id="triangle1" type="triangle" args="-0.2 0 0 -1.3 0 0 0 0 3" />
        <LEAF id="triangle2" type="triangle" args="0 -0.5 0 -0.2 0 0 0 0 3" />
        <LEAF id="triangle3" type="triangle" args="0.2 0 0 0 -0.5 0 0 0 3" />
        <LEAF id="triangle4" type="triangle" args="1.3 0 0 0.2 0 0 0 0 3" />
		<LEAF id="lamp_default_top" type="cylinder" args="0.7 0.6 0.4 20 20" />
		<LEAF id="lamp_default_middle" type="cylinder" args="3 0.025 0.025 20 20" />
		<LEAF id="lamp_default_bottom" type="cylinder" args="0.05 0.25 0.25 20 20" />
		<LEAF id="flag_pole_leaf" type="cylinder" args="5 0.025 0.025 20 20" />
		<LEAF id="patch" type="patch" order="3" partsU="20" partsV="20">           
			<CONTROLPOINT x="-9" y="-6" z="0" w="1" />
			<CONTROLPOINT x="-9" y="-2" z="0" w="1" />
			<CONTROLPOINT x="-9" y="2" z="0" w="1" />
			<CONTROLPOINT x="-9" y="6" z="0" w="1" />
			<CONTROLPOINT x="-3" y="-6" z="4" w="1" />
			<CONTROLPOINT x="-3" y="-2" z="4" w="1" />
			<CONTROLPOINT x="-3" y="2" z="4" w="1" />
			<CONTROLPOINT x="-3" y="6" z="4" w="1" />
			<CONTROLPOINT x="3" y="-6" z="-4" w="1" />
			<CONTROLPOINT x="3" y="-2" z="-4" w="1" />
			<CONTROLPOINT x="3" y="2" z="-4" w="1" />
			<CONTROLPOINT x="3" y="6" z="-4" w="1" />
			<CONTROLPOINT x="9" y="-6" z="0" w="1" />
			<CONTROLPOINT x="9" y="-2" z="0" w="1" />
			<CONTROLPOINT x="9" y="2" z="0" w="1" />
			<CONTROLPOINT x="9" y="6" z="0" w="1" />
		</LEAF>
		<LEAF id="grass_leaf" type="terrain" texture="textures/grass.jpg" heightmap="textures/grass_heightmap.jpg" />
	</LEAVES>


	<NODES>
		<ROOT id="scene" />

	    <NODE id="scene">
			<MATERIAL id="null" />
			<TEXTURE id="null" />
			<DESCENDANTS>
				<DESCENDANT id="environment" />
				<DESCENDANT id="flag" />
				<DESCENDANT id="paper_plane" />
				<DESCENDANT id="lamp" />
				<DESCENDANT id="chair" />
				<DESCENDANT id="paintings" />
			</DESCENDANTS>
		</NODE>
		
		<NODE id="environment">
			<MATERIAL id="null" />
			<TEXTURE id="null" />
			<DESCENDANTS>
				<DESCENDANT id="walls" />
				<DESCENDANT id="floor" />
			</DESCENDANTS>
		</NODE>

		<NODE id="walls">
			<MATERIAL id="default" />
			<TEXTURE id="wall_pattern" />
			<DESCENDANTS>
				<DESCENDANT id="wall_front" />
				<DESCENDANT id="wall_side" />
			</DESCENDANTS>
		</NODE>

		<NODE id="wall_side">
			<MATERIAL id="null" />
			<TEXTURE id="null" />	
			<ROTATION axis="y" angle="-90"/>
			<SCALE sx="10" sy="6" sz="0"/>
			<DESCENDANTS>
				<DESCENDANT id="rectangle" />
			</DESCENDANTS>
		</NODE>

		<NODE id="wall_front">
			<MATERIAL id="null" />
			<TEXTURE id="null" />
			<TRANSLATION x="10" y="0" z="0" />
			<ROTATION axis="y" angle="180"/>
			<SCALE sx="10" sy="6" sz="0"/>
			<DESCENDANTS>
				<DESCENDANT id="rectangle" />
			</DESCENDANTS>
		</NODE>

		<NODE id="floor">
			<MATERIAL id="null" />
			<TEXTURE id="null" />
			<DESCENDANTS>
				<DESCENDANT id="actual_floor" />
				<DESCENDANT id="grass" />											
			</DESCENDANTS>
		</NODE>

		<NODE id="actual_floor">
			<MATERIAL id="default" />
			<TEXTURE id="floor_pattern" />
			<ROTATION axis="x" angle="90"/>
			<SCALE sx="10" sy="10" sz="0"/>
			<DESCENDANTS>
				<DESCENDANT id="rectangle" />												
			</DESCENDANTS>
		</NODE>

		<NODE id="grass">
			<MATERIAL id="default" />
			<TEXTURE id="grass" />
			<TRANSLATION x="5" y="0.0001" z="5" />
			<SCALE sx="10" sy="0.35" sz="10"/>
			<DESCENDANTS>
				<DESCENDANT id="grass_leaf" />												
			</DESCENDANTS>
		</NODE>

		<NODE id="flag">
			<MATERIAL id="null" />
			<TEXTURE id="null" />
			<TRANSLATION x="8.1" y="0" z="0.5" />
			<DESCENDANTS>
				<DESCENDANT id="flag_pole" />
				<DESCENDANT id="actual_flag" />
			</DESCENDANTS>
		</NODE>

		<NODE id="flag_pole">
			<MATERIAL id="default" />
			<TEXTURE id="steel" />			
			<ROTATION axis="x" angle="-90"/>
			<DESCENDANTS>
				<DESCENDANT id="flag_pole_leaf" />
			</DESCENDANTS>
		</NODE>

		<NODE id="actual_flag">
			<MATERIAL id="default" />
			<TEXTURE id="flag" />
			<ANIMATIONREF id="flag_animation" />
			<TRANSLATION x="0.775" y="4.4" z="0" />
			<SCALE sx="0.0833" sy="0.0833" sz="0.0833"/>			
			<DESCENDANTS>
				<DESCENDANT id="patch" />
			</DESCENDANTS>
		</NODE>

		<NODE id="paper_plane">
			<MATERIAL id="default" />
			<TEXTURE id="paper" />
			<ANIMATIONREF id="paperPlane_animation" />
			<ANIMATIONREF id="circular" />
			<TRANSLATION x="2" y="3" z="8" />
			<ROTATION axis="y" angle="180"/>
			<SCALE sx="0.4" sy="0.4" sz="0.4"/>
			<DESCENDANTS>
				<DESCENDANT id="wing1" />
				<DESCENDANT id="wing2" />
				<DESCENDANT id="wing_bottom1" />
				<DESCENDANT id="wing_bottom2" />
			</DESCENDANTS>
		</NODE>

		<NODE id="wing1">
			<MATERIAL id="null" />
			<TEXTURE id="null" />		
			<DESCENDANTS>
				<DESCENDANT id="triangle1" />
			</DESCENDANTS>
		</NODE>

		<NODE id="wing2">
			<MATERIAL id="null" />
			<TEXTURE id="null" />		
			<DESCENDANTS>
				<DESCENDANT id="triangle4" />
			</DESCENDANTS>
		</NODE>

		<NODE id="wing_bottom1">
			<MATERIAL id="null" />
			<TEXTURE id="null" />		
			<DESCENDANTS>
				<DESCENDANT id="triangle2" />
			</DESCENDANTS>
		</NODE>

		<NODE id="wing_bottom2">
			<MATERIAL id="null" />
			<TEXTURE id="null" />		
			<DESCENDANTS>
				<DESCENDANT id="triangle3" />
			</DESCENDANTS>
		</NODE>

		<NODE id="lamp">
			<MATERIAL id="null" />
			<TEXTURE id="null" />
			<TRANSLATION x="0.7" y="0" z="0.7" />
			<DESCENDANTS>
				<DESCENDANT id="lamp_support" />
				<DESCENDANT id="lamp_top" />			
			</DESCENDANTS>
		</NODE>

		<NODE id="lamp_top">
			<MATERIAL id="default" />
			<TEXTURE id="lamp_top_pattern" />
			<TRANSLATION x="0" y="3.05" z="0" />
			<ROTATION axis="x" angle="-90"/>
			<DESCENDANTS>
				<DESCENDANT id="lamp_default_top" />
			</DESCENDANTS>
		</NODE>

		<NODE id="lamp_support">
			<MATERIAL id="default" />
			<TEXTURE id="lamp_support_pattern" />
			<DESCENDANTS>
				<DESCENDANT id="lamp_middle" />
				<DESCENDANT id="lamp_bottom" />	
			</DESCENDANTS>
		</NODE>

		<NODE id="lamp_middle">
			<MATERIAL id="null" />
			<TEXTURE id="null" />
			<TRANSLATION x="0" y="0.05" z="0" />
			<ROTATION axis="x" angle="-90"/>
			<DESCENDANTS>
				<DESCENDANT id="lamp_default_middle" />
			</DESCENDANTS>
		</NODE>

		<NODE id="lamp_bottom">
			<MATERIAL id="null" />
			<TEXTURE id="null" />
			<TRANSLATION x="0" y="0.05" z="0" />
			<ROTATION axis="x" angle="90"/>
			<DESCENDANTS>
				<DESCENDANT id="lamp_default_bottom" />
			</DESCENDANTS>
		</NODE>

		<NODE id="chair">
			<MATERIAL id="default" />
			<TEXTURE id="chair_pattern" />
			<TRANSLATION x="4.5" y="0" z="4.5" />
			<DESCENDANTS>
				<DESCENDANT id="chair_seat" />
				<DESCENDANT id="chair_back" />
				<DESCENDANT id="chair_legs" />
			</DESCENDANTS>
		</NODE>

		<NODE id="chair_seat">
			<MATERIAL id="null" />
			<TEXTURE id="null" />
			<TRANSLATION x="0" y="0.8" z="0" />
			<SCALE sx="1" sy="0.05" sz="1"/>
			<DESCENDANTS>
				<DESCENDANT id="cube" />
			</DESCENDANTS>
		</NODE>

		<NODE id="chair_back">
			<MATERIAL id="null" />
			<TEXTURE id="null" />
			<TRANSLATION x="0" y="0.85" z="0.90" />
			<SCALE sx="1" sy="1.25" sz="0.1"/>
			<DESCENDANTS>
				<DESCENDANT id="cube" />
			</DESCENDANTS>
		</NODE>

		<NODE id="chair_legs">
			<MATERIAL id="null" />
			<TEXTURE id="null" />
			<SCALE sx="0.1" sy="0.8" sz="0.1"/>
			<DESCENDANTS>
				<DESCENDANT id="chair_leg_1" />
				<DESCENDANT id="chair_leg_2" />
				<DESCENDANT id="chair_leg_3" />
				<DESCENDANT id="chair_leg_4" />
			</DESCENDANTS>
		</NODE>

		<NODE id="chair_leg_1">
			<MATERIAL id="null" />
			<TEXTURE id="null" />
			<DESCENDANTS>
				<DESCENDANT id="cube" />
			</DESCENDANTS>
		</NODE>

		<NODE id="chair_leg_2">
			<MATERIAL id="null" />
			<TEXTURE id="null" />
			<TRANSLATION x="9" y="0" z="0" />
			<DESCENDANTS>
				<DESCENDANT id="cube" />
			</DESCENDANTS>
		</NODE>

		<NODE id="chair_leg_3">
			<MATERIAL id="null" />
			<TEXTURE id="null" />
			<TRANSLATION x="0" y="0" z="9" />
			<DESCENDANTS>
				<DESCENDANT id="cube" />
			</DESCENDANTS>
		</NODE>

		<NODE id="chair_leg_4">
			<MATERIAL id="null" />
			<TEXTURE id="null" />
			<TRANSLATION x="9" y="0" z="9" />
			<DESCENDANTS>
				<DESCENDANT id="cube" />
			</DESCENDANTS>
		</NODE>

		<NODE id="cube">
			<MATERIAL id="null" />
			<TEXTURE id="null" />
			<DESCENDANTS>
	           	<DESCENDANT id="face_top" />
	           	<DESCENDANT id="face_bot" />
	           	<DESCENDANT id="face_left" />
	           	<DESCENDANT id="face_right" />
	           	<DESCENDANT id="face_back" />
	           	<DESCENDANT id="face_front" />
	       	</DESCENDANTS>
		</NODE>

  		<NODE id="face_top">
      		<MATERIAL id="null" />
      		<TEXTURE id="null" />
      		<TRANSLATION x="0" y="1" z="0" />
      		<ROTATION axis="x" angle="90"/>
      		<DESCENDANTS>
      		    <DESCENDANT id="rectangle" />
      		</DESCENDANTS>
  		</NODE>

	  	<NODE id="face_bot">
	      	<MATERIAL id="null" />
	      	<TEXTURE id="null" />
	      	<TRANSLATION x="0" y="0" z="1" />
	      	<ROTATION axis="x" angle="-90"/>
	      	<DESCENDANTS>
	          	<DESCENDANT id="rectangle" />
	      	</DESCENDANTS>
	  	</NODE>

	  	<NODE id="face_left">
	      	<MATERIAL id="null" />
	      	<TEXTURE id="null" />
	      	<TRANSLATION x="0" y="0" z="1" />
	      	<ROTATION axis="y" angle="90"/>
	      	<DESCENDANTS>
	          	<DESCENDANT id="rectangle" />
	      	</DESCENDANTS>
	  	</NODE>

	  	<NODE id="face_right">
	     	<MATERIAL id="null" />
	    	<TEXTURE id="null" />
	      	<TRANSLATION x="1" y="0" z="0" />
	      	<ROTATION axis="y" angle="-90"/>
	      	<DESCENDANTS>
	          	<DESCENDANT id="rectangle" />
	      	</DESCENDANTS>
	  	</NODE>

	  	<NODE id="face_back">
	      	<MATERIAL id="null" />
	      	<TEXTURE id="null" />
	      	<TRANSLATION x="0" y="0" z="0" />
	      	<DESCENDANTS>
	          	<DESCENDANT id="rectangle" />
	      	</DESCENDANTS>
	  	</NODE>

	  	<NODE id="face_front">
	      	<MATERIAL id="null" />
	      	<TEXTURE id="null" />
	      	<TRANSLATION x="0" y="1" z="1" />
	      	<ROTATION axis="x" angle="180"/>
	      	<DESCENDANTS>
	          	<DESCENDANT id="rectangle" />
	      	</DESCENDANTS>
	  	</NODE>

	  	<NODE id="paintings">
			<MATERIAL id="null" />
			<TEXTURE id="null" />
			<DESCENDANTS>
				<DESCENDANT id="o_grito" />
				<DESCENDANT id="mona_lisa" />
				<DESCENDANT id="persistencia_da_memoria" />
			</DESCENDANTS>
		</NODE>

		<NODE id="o_grito">
			<MATERIAL id="default" />
			<TEXTURE id="painting1" />
			<TRANSLATION x="0.001" y="1.5" z="5.9" />
			<SCALE sx="0" sy="3" sz="2.5"/>
			<ROTATION axis="y" angle="-90"/>
			<DESCENDANTS>
				<DESCENDANT id="rectangle" />				
			</DESCENDANTS>
		</NODE>

		<NODE id="mona_lisa">
			<MATERIAL id="default" />
			<TEXTURE id="painting2" />
			<TRANSLATION x="0.001" y="1.5" z="1.6" />
			<SCALE sx="0" sy="3" sz="2.5"/>
			<ROTATION axis="y" angle="-90"/>
			<DESCENDANTS>
				<DESCENDANT id="rectangle" />				
			</DESCENDANTS>
		</NODE>

		<NODE id="persistencia_da_memoria">
			<MATERIAL id="default" />
			<TEXTURE id="painting3" />
			<TRANSLATION x="7.5" y="1.5" z="0.001" />
			<SCALE sx="5" sy="3" sz="0"/>
			<ROTATION axis="y" angle="180"/>
			<DESCENDANTS>
				<DESCENDANT id="rectangle" />				
			</DESCENDANTS>
		</NODE>

	</NODES>	

	
</SCENE>
